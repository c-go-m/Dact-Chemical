<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="httpplatformhandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified" />
        </handlers>
        <httpPlatform processPath="%HOME%\site\wwwroot\dc-admin" arguments="" stdoutLogEnabled="true" stdoutLogFile="%HOME%\LogFiles\stdout.log" startupTimeLimit="300">
            <environmentVariables>
                <environmentVariable name="ASPNETCORE_ENVIRONMENT" value="Production" />
            </environmentVariables>
        </httpPlatform>
    </system.webServer>
</configuration>